{
  "name": "toggl-summary",
  "version": "1.0.0",
  "repository": "git@github.com:pthorsson/toggl-summary.git",
  "author": "pthorsson <p.thorsson@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean:dist && webpack --env.prod",
    "dev": "yarn clean:dist && npm-run-all --parallel dev:watch dev:server",
    "dev:watch": "webpack --env.dev",
    "dev:server": "live-server ./dist --port=9001 --entry-file=./dist/index.html --middleware=spa-ignore-assets",
    "clean:dist": "rm -rf dist && cp -r static dist"
  },
  "dependencies": {
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/styled-components": "^4.1.12",
    "lodash": "^4.17.11",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^5.0.1",
    "live-server": "^1.2.1",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "yarn-run-all": "^3.1.1"
  }
}
